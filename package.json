{
  "name": "my-jsx",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf dist && mkdir dist",
    "serve:static": "npx babel ./static --out-dir ./dist --copy-files --watch",
    "serve:jsx": "npx babel ./src --out-dir ./dist --plugins @babel/plugin-syntax-jsx --watch",
    "serve:http": "npx http-server ./dist",
    "start": "npm run clean && npm-run-all --parallel serve:*"
  },
  "author": "acrm <github.com/acrm>",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.17.10",
    "@babel/plugin-transform-react-jsx": "^7.17.3",
    "@babel/preset-env": "^7.17.10",
    "http-server": "^14.1.0",
    "npm-run-all": "^4.1.5"
  }
}
